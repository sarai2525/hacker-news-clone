{
  "name": "hacker news clone",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:script": "eslint --fix --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{.js,ts,vue}": "pnpm lint:script",
    "*.{md,json,yml,yaml}": "pnpm lint:prettier"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "vite",
        "webpack"
      ]
    }
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.5",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18",
    "@typescript-eslint/parser": "^5.59.7",
    "@vue/eslint-config-prettier": "^7.1.0",
    "element-plus": "^2.3.5",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.1",
    "ofetch": "^1.0.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
